<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.teamNIKaml.locationReminder.activity"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="19"
        android:targetSdkVersion="19" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".LauncherActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".AddLocationActivity"
            android:label="@string/title_activity_add_location" >
        </activity>
        <activity
            android:name=".DeletePlaceActivity"
            android:label="@string/title_activity_delete_place" >
        </activity>
        <activity
            android:name=".LocationUpdateActivit"
            android:label="@string/title_activity_location_update" >
        </activity>
        <activity
            android:name=".LocationUpdateActivity"
            android:label="@string/title_activity_location_update" >
        </activity>
    </application>

</manifest>
